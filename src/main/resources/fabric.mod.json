{
  "schemaVersion": 1,
  "id": "multiblocklib",
  "version": "${version}",
  "name": "Multiblock Lib",
  "description": "A library for multiblocks!",
  "authors": [
    "Jamalam360 - Developer"
  ],
  "contact": {
    "homepage": "https://github.com/JamCoreModding/MultiblockLib",
    "issues": "https://github.com/JamCoreModding/MultiblockLib/issues",
    "sources": "https://github.com/JamCoreModding/MultiblockLib"
  },
  "license": "MIT",
  "icon": "assets/multiblocklib/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "io.github.jamalam360.MultiblockEntrypoint"
    ],
    "cardinal-components-world": [
      "io.github.jamalam360.components.CardinalComponentsInit"
    ]
  },
  "mixins": [
    "multiblocklib.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.12.5",
    "fabric": "*"
  },
  "custom": {
    "notify_gradle_properties_url": "https://raw.githubusercontent.com/JamCoreModding/MultiblockLib/main/gradle.properties",
    "notify_gradle_properties_key": "modVersion",
    "cardinal-components": [
      "multiblocklib:multiblock_provider"
    ]
  }
}
